{
  "root": "apps/micro-fe/solid-container",
  "sourceRoot": "apps/micro-fe/solid-container/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/micro-fe/solid-container",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "dist/apps/micro-fe/solid-container",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/micro-fe/solid-container/**/*.{ts,spec.ts,tsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/micro-fe/solid-container"],
      "options": {
        "jestConfig": "apps/micro-fe/solid-container/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
