{
  "root": "apps/fullstack/fullstack-webclient-playwright",
  "sourceRoot": "apps/fullstack/fullstack-webclient-playwright/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@ns3/nx-jest-playwright:jest-playwright",
      "options": {
        "devServerTarget": "fullstack-webclient:serve",
        "jestConfig": "apps/fullstack/fullstack-webclient-playwright/jest.config.js",
        "passWithNoTests": true
      },
      "configurations": {
        "production": {
          "devServerTarget": "fullstack-webclient:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/fullstack/fullstack-webclient-playwright/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["fullstack-webclient"]
}
