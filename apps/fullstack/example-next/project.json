{
  "root": "apps/fullstack/example-next",
  "sourceRoot": "apps/fullstack/example-next",
  "projectType": "application",
  "targets": {
    "builds": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/fullstack/example-next",
        "outputPath": "apps/fullstack/example-next/dist"
      },
      "configurations": {
        "production": {}
      }
    },
    "serves": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "fullstack-example-next:build",
        "dev": true,
        "open": true
      },
      "configurations": {
        "production": {
          "buildTarget": "fullstack-example-next:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "fullstack-example-next:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/fullstack/example-next"],
      "options": {
        "jestConfig": "apps/fullstack/example-next/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fullstack/example-next/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
